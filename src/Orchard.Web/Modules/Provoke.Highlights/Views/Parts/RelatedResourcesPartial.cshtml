@using Newtonsoft.Json
@using Provoke.Highlights.ViewModels






@foreach (var link in Model.RelatedResources) {
   
    <div class="col-md-3 single-resource">
            @{ var iconClass = "icon_" + link.Type + "_codeSample"; }

            <div class="">
                <a href="@link.Url">

                    <div class="@iconClass link-icon col-xs-3"></div>
                    <div class="link-title col-xs-9">@link.Title</div>
                </a>
            </div>
    </div>
}