@using Orchard.Utility.Extensions;
@{
    if (Model.Title != null)
    {
        Layout.Title = Model.Title;
    }

    Model.Classes.Add("content-item");

    var contentTypeClassName = ((string)Model.ContentItem.ContentType).HtmlClassify();
    Model.Classes.Add(contentTypeClassName);

    var gettingStartedItem = Model.ContentItem;

  
}

<li class="col-md-6 col-sm-6 collapse event-box-details" role="listitem" aria-label="@gettingStartedItem.TitlePart.Title">
    <div class="code-event">
        <div class="event-info">

            <div class="code-title">
                <div class="event-icon col-xs-4">
                  
                </div>
                <div class="col-xs-8 name cp1">@gettingStartedItem.TitlePart.Title</div>
                <div class="col-xs-8 event-links">
                    <a data-bind="attr: { href: Url }" href="/getting-started" role="link">VIEW <i class="icon_view"></i> </a>
                    <a href="#" class="more"><p>MORE INFO</p> <i class="icon_plus"></i></a>

                </div>
            </div>
        </div>
        <div class="register-now">
            <div class="desc"></div>
        </div>

    </div>
</li>